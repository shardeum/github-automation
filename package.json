{
  "name": "test-project",
  "version": "1.0.0",
  "description": "A sample project to test github automation",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "compile": "tsc -p .",
    "lint": "eslint \"./src/**/*.ts\"",
    "format-check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "format-fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "test": "jest",
    "test:istanbul": "nyc jest",
    "test:coverage": "jest --coverage --coverageReporters=\"text-summary\" --coverageReporters=\"json-summary\"",
    "posttest": "jest-it-up",
    "clean": "rimraf dist"
  },
  "author": "Shardeum Systems & Automation",
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-xss": "0.1.12",
    "globals": "15.3.0",
    "jest": "29.7.0",
    "jest-it-up": "3.2.0",
    "nyc": "17.0.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.7",
    "ts-jest": "29.2.4",
    "ts-node": "10.9.2",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "express": "4.17.1"
  }
}
